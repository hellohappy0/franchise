{
  "name": "franchise",
  "version": "1.0.0",
  "description": "a sql notebook",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app src/index.js bundle --no-vendor",
    "clean": "nwb clean-app",
    "start": "nwb serve-react-app",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "deploy": "gh-pages -d bundle",
    "pack": "electron-builder --dir",
    "dist": "yarn build && electron-builder",
    "build:electron:linux": "yarn build &&  electron-builder build --linux",
    "build:electron:mac": "yarn build &&  electron-builder build --mac",
    "build:electron:windows": "yarn build &&  electron-builder build --windows"
  },
  "dependencies": {
    "franchise-client": "^0.2.7",
    "@blueprintjs/core": "~1.22.0",
    "@blueprintjs/labs": "~0.5.0",
    "@blueprintjs/table": "~1.19.0",
    "@skidding/react-codemirror": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "breadloaf": "^1.3.9",
    "chart.js": "^2.6.0",
    "chartist": "^0.11.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.28.0",
    "d3-shape": "^1.2.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.4.0",
    "lodash": "^4.17.4",
    "mingo": "^1.3.1",
    "normalize.css": "^5.0.0",
    "raw-loader": "^0.5.1",
    "rc-tabs": "^7.1.1",
    "react": "^15.4.2",
    "react-collapse": "^4.0.2",
    "react-dom": "^15.4.2",
    "react-leaflet": "^1.4.0",
    "react-motion": "^0.5.0",
    "sql-formatter": "^2.0.0",
    "sql.js": "^0.4.0",
    "sqlgenerate": "https://github.com/jdrew1303/sqlgenerate",
    "sqlite-parser": "^1.0.1",
    "stats-webpack-plugin": "^0.6.1",
    "sweetalert-react": "^0.4.10",
    "sweetalert2": "^6.6.6",
    "tsparser": "^1.0.2",
    "uglify-loader": "^2.0.0",
    "url-loader": "^0.5.9",
    "whatwg-fetch": "^2.0.3",
    "worker-loader": "^0.8.1",
    "xlsx": "^0.11.6",
    "ws": "^6.1.4",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "gh-pages": "^1.0.0",
